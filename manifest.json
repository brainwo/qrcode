{
  "manifest_version": 2,
  "name": "__MSG_name__",
  "version": "1.3.8.2",
  "short_name": "__MSG_short_name__",
  "default_locale": "en",
  "description": "__MSG_description__",
  "author": "Cople",
  "icons": {
    "16": "icon/icon.svg",
    "32": "icon/icon.svg",
    "48": "icon/icon.svg",
    "96": "icon/icon.svg",
    "128": "icon/icon.svg"
  },
  "page_action": {
    "default_icon": {
      "16": "icon/icon.svg",
      "19": "icon/icon.svg",
      "32": "icon/icon.svg",
      "38": "icon/icon.svg"
    },
    "default_title": "__MSG_generate__",
    "default_popup": "popup.html"
  },
  "commands": {
    "_execute_page_action": {
      "suggested_key": {
        "default": "Alt+Shift+Q"
      }
    }
  },
  "background": {
    "scripts": [
      "js/vendor/qrcode.js",
      "js/vendor/qrcode_UTF8.js",
      "js/noconflict.js",
      "js/vendor/jsqrcode/grid.js",
      "js/vendor/jsqrcode/version.js",
      "js/vendor/jsqrcode/detector.js",
      "js/vendor/jsqrcode/formatinf.js",
      "js/vendor/jsqrcode/errorlevel.js",
      "js/vendor/jsqrcode/bitmat.js",
      "js/vendor/jsqrcode/datablock.js",
      "js/vendor/jsqrcode/bmparser.js",
      "js/vendor/jsqrcode/datamask.js",
      "js/vendor/jsqrcode/rsdecoder.js",
      "js/vendor/jsqrcode/gf256poly.js",
      "js/vendor/jsqrcode/gf256.js",
      "js/vendor/jsqrcode/decoder.js",
      "js/vendor/jsqrcode/qrcode.js",
      "js/vendor/jsqrcode/findpat.js",
      "js/vendor/jsqrcode/alignpat.js",
      "js/vendor/jsqrcode/databr.js",
      "js/background.js"
    ]
  },
  "options_ui": {
    "browser_style": true,
    "page": "options.html"
  },
  "permissions": ["contextMenus", "activeTab", "<all_urls>"]
}
